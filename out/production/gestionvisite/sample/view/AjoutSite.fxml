<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="257.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.AjoutSiteController">
    <children>
        <GridPane layoutX="50.0" layoutY="31.0" prefHeight="136.0" prefWidth="501.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Nom Site" />
                <Label text="Lieu" GridPane.rowIndex="1" />

                <TextField fx:id="Nom" GridPane.columnIndex="1" />
                <TextField fx:id="Lieu" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Tarif" GridPane.rowIndex="2" />
            <TextField fx:id="Tarif" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            </children>
        </GridPane>
        <ButtonBar layoutX="166.0" layoutY="178.0" prefHeight="40.0" prefWidth="250.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#AjouterSite" text="Enregistrer" />
                <Button layoutX="135.0" layoutY="18.0" mnemonicParsing="false" onAction="#annuler" prefHeight="25.0" prefWidth="133.0" text="Annuler" textAlignment="JUSTIFY" />
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>

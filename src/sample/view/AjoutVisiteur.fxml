<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="208.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.AjoutVisiteurController">
   <children>
      <GridPane layoutX="47.0" layoutY="55.0" prefHeight="84.0" prefWidth="501.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Nom Visiteur" />
            <Label text="Adresse" GridPane.rowIndex="1" />
            <TextField fx:id="Nom" GridPane.columnIndex="1" />
            <TextField fx:id="Adresse" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <ButtonBar layoutX="173.0" layoutY="146.0" prefHeight="40.0" prefWidth="250.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#AjouterVisiteur" text="Enregistrer" />
            <Button layoutX="135.0" layoutY="18.0" mnemonicParsing="false" onAction="#annuler" prefHeight="25.0" prefWidth="133.0" text="Annuler" textAlignment="JUSTIFY" />
        </buttons>
      </ButtonBar>
   </children>
